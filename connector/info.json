{
    "name": "sample-connector",
    "version": "1.0.0",
    "label": "Sample Connector",
    "description": "Naming convention.\nTitle should be change from \"Sample Connector\" --> \"Organization ProductName\"\nAPI name should change from \"sample-connector\" --> \"org-productName\"",
    "publisher": "anonyges@gmail.com",
    "icon_large_name": "connector_logo_large.png",
    "icon_small_name": "connector_logo_small.png",
    "category": [
        "Analytics and SIEM"
    ],
    "configuration": {
        "fields": [
            {
                "title": "URL",
                "type": "text",
                "name": "url",
                "required": true,
                "visible": true,
                "editable": true,
                "validation": {
                    "pattern": "^https?:\\/\\/(?:(?:(?:[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?\\.)+[a-z0-9][a-z0-9-]{0,61}[a-z0-9])|(?:(?:(?:(?!25?[6-9])[12]\\d|[1-9])?\\d\\.?\\b){4})|(?:(?:[a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*(?:[A-Za-z0-9]|[A-Za-z0-9][A-Za-z0-9\\-]*[A-Za-z0-9]))(?::\\d+)?$",
                    "patternError": "Server URL must begin with https? and end without '/'. Port number can be added eg) <https://example.com:80>."
                }
            },
            {
                "title": "API Key",
                "type": "password",
                "name": "api_key",
                "required": true,
                "visible": true,
                "editable": true
            },
            {
                "title": "Verify SSL",
                "type": "checkbox",
                "name": "verify_ssl",
                "required": false,
                "visible": true,
                "editable": true,
                "value": true
            }
        ]
    },
    "operations": [
        {
            "operation": "generic_api_call",
            "title": "Generic API call",
            "description": "Makes API call to the given endpoint.",
            "enabled": true,
            "category": "",
            "annotation": "",
            "TEMPLATE_NOTE6": "--OUTPUT SCHEMA IS A SAMPLE RESPONSE FROM THIS CONNECTOR ACTION--",
            "output_schema": {
                "result": "",
                "api_data": ""
            },
            "parameters": [
                {
                    "title": "api_endpoint",
                    "type": "text",
                    "name": "api_endpoint",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "value": "",
                    "tooltip": "Should include the whole string eg) \"/api/v1/helloworld\""
                },
                {
                    "title": "method",
                    "type": "select",
                    "name": "method",
                    "required": true,
                    "visible": true,
                    "editable": true,
                    "value": "GET",
                    "options": [
                        "GET",
                        "PUT",
                        "POST",
                        "DELETE",
                        "PATCH",
                        "HEAD",
                        "OPTIONS",
                        "TRACE"
                    ]
                },
                {
                    "title": "params",
                    "type": "object",
                    "name": "params",
                    "required": false,
                    "visible": true,
                    "editable": true,
                    "value": null,
                    "tooltip": "Same as requests library \"params\" will be converted to get in URL"
                },
                {
                    "title": "data",
                    "type": "object",
                    "name": "data",
                    "required": false,
                    "visible": true,
                    "editable": true,
                    "value": null,
                    "tooltip": "Same as requests library \"json_data\" will be converted to get in Body"
                },
                {
                    "title": "headers",
                    "type": "object",
                    "name": "headers",
                    "required": false,
                    "visible": false,
                    "editable": true,
                    "value": null,
                    "tooltip": "Append value in requests headers when headers is set."
                }
            ]
        },
        {
            "operation": "do_something",
            "title": "Do Something",
            "description": "Do Something and Returns Something.",
            "enabled": true,
            "open": false
        },
        {
            "operation": "raise_error",
            "title": "Raise Error",
            "description": "Raise Error!",
            "enabled": true,
            "open": false
        }
    ]
}